<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/DECORATION/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0 http://maven.apache.org/xsd/decoration-1.0.0.xsd">

    <skin>
		<groupId>com.icfolson.maven</groupId>
		<artifactId>icfolson-maven-skin</artifactId>
		<version>1.1.0</version>
	</skin>

    <bannerRight>
		<name>OSGi Bundle Status Maven Plugin</name>
		<href>index.html</href>
	</bannerRight>

    <googleAnalyticsAccountId>UA-928005-4</googleAnalyticsAccountId>

    <custom>
        <reflowSkin>
            <skinAttribution>false</skinAttribution>
            <protocolRelativeURLs>false</protocolRelativeURLs>
            <smoothScroll>true</smoothScroll>
            <titleTemplate>%2$s | %1$s</titleTemplate>
            <breadcrumbs>false</breadcrumbs>
            <markPageHeader>false</markPageHeader>
            <toc>false</toc>
            <highlightJs>true</highlightJs>
            <highlightJsTheme>github</highlightJsTheme>
            <topNav>Overview|Examples|reports</topNav>
            <brand>
                <name>${project.name}</name>
                <href>index.html</href>
            </brand>
            <slogan position="bannerRight">Remote bundle status checker for OSGi containers.</slogan>
            <body />
            <bottomNav>
                <column>Overview</column>
                <column>Examples</column>
                <column>reports</column>
            </bottomNav>
        </reflowSkin>
    </custom>

    <body>
        <links>
            <item name="GitHub" href="https://github.com/Citytechinc/osgi-bundle-status-maven-plugin" />
        </links>
        <menu name="Overview">
            <item name="Introduction" href="index.html"/>
            <item name="Goals" href="plugin-info.html"/>
            <item name="Usage" href="usage.html"/>
        </menu>
        <menu name="Examples">
            <item name="Basic" href="basic.html"/>
            <item name="Advanced" href="advanced.html"/>
            <item name="Skipping Verification" href="skip.html"/>
        </menu>
        <menu ref="reports"/>
    </body>
</project>